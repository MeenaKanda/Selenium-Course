			<class name="testNGHW1.LoginPageTest" />
		</classes>
	</test>
	
	
	<test name="Register Page Test">
	<parameter name="url" value="https://naveenautomationlabs.com/opencart/index.php?route=account/register" />
   <parameter name="browser" value="chrome" />

		<classes>
			<class name="testNGHW1.RegisterPageTest" />
		</classes>
	</test>
	
	<test name="Forgot Password Test">
	<parameter name="url" value="https://naveenautomationlabs.com/opencart/index.php?route=account/forgotten" />
   <parameter name="browser" value="chrome" />

		<classes>
			<class name="testNGHW1.ForgotPasswordTest" />
		</classes>
	</test>


</suite>


